  "version": "0.0.1",
  "keywords": [
    "Shopify",
    "eCommerce",
    "Chrome",
    "Google"
  ],
  "maintainers": [
    {
      "name": "Liam Merlyn",
      "email": "l.merlyn@hotmail.co.uk",
      "web": "https://github.com/ConduciveMammal"
    }
  ],
  "bugs": {
    "web": "https://github.com/ConduciveMammal/storeexplorer/issues"
  },
  "repositories": [
    {
      "type": "git",
