{
  "extends": "eslint-config-accurapp",
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": "writable"
  },
  "rules": {
    "generator-star-spacing": "off",
    "no-multiple-empty-lines": "off",
    "no-bitwise": "off",
    "no-useless-escape": "off",
    "react/jsx-no-bind": "off",
    "react/no-unknown-property": ["warn", {"ignore": ["class", "for"]}]
  }
}
